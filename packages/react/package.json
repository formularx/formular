{
  "name": "@formular/react",
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "author": "HeskeyBaozi <hezhiyu233@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel examples/basic/index.html --open",
    "lint": "eslint --ext .ts,.tsx src",
    "build": "npm run build:es && npm run build:lib",
    "build:es": "rimraf es && cross-env ESMODULE=1 babel src --out-dir es --extensions '.ts,.tsx' && tsc --emitDeclarationOnly --outDir es",
    "build:lib": "rimraf lib && babel src --out-dir lib --extensions '.ts,.tsx' && tsc --emitDeclarationOnly --outDir lib",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@formular/core": "^1.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.memoize": "^4.1.6",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2"
  },
  "peerDependencies": {
    "mobx": "5.x",
    "mobx-react": "6.x",
    "mobx-state-tree": "3.x",
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "mobx-react": "^6.1.8",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "homepage": "https://github.com/formularx/formular#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formularx/formular.git"
  },
  "bugs": {
    "url": "https://github.com/formularx/formular/issues"
  },
  "publishConfig": {
    "access": "publish"
  }
}
